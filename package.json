{
    "name": "front-end-framework",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "gulp",
        "prod": "npm start -- --production",
        "webpack": "gulp js",
        "t4": "webpack --config=\"./build/tasks/webpack/t4.webpack.config.js\" --production"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "browserslist": [
        ">0.5%",
        "ie >= 11",
        "safari >= 10",
        "last 3 iOS versions",
        "last 3 Chrome versions",
        "last 3 Firefox versions",
        "last 3 Edge versions"
    ],
    "stylelint": {
        "extends": [
            "@uwhealth/stylelint-config"
        ]
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "@uwhealth"
        ],
        "plugins": [
            "@uwhealth/environments"
        ],
        "env": {
            "@uwhealth/environments/webpack": true,
            "es6": true
        },
        "overrides": [
            {
                "files": [
                    "**/*.js"
                ],
                "rules": {
                    "no-console": "off"
                }
            }
        ]
    },
    "eslintIgnore": [
        "node_modules/*",
        "dist/*"
    ],
    "devDependencies": {
