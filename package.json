{
    "name": "front-end-framework",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "start": "npx gulp",
        "gulp": "node build/gulp.build.js",
        "prod": "npm start -- --production",
        "webpack": "npx gulp js",
        "t4": "npx webpack --config=\"./build/webpack/t4.webpack.config.js\" --production"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "browserslist": [
        ">2%",
        "safari >= 10",
        "last 3 iOS versions",
        "last 3 Chrome versions",
        "last 3 Firefox versions",
        "last 3 Edge versions"
    ],
    "stylelint": {
        "extends": [
            "@uwhealth/stylelint-config"
        ]
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "@uwhealth"
        ],
        "plugins": [
            "@uwhealth/environments"
        ],
        "env": {
            "@uwhealth/environments/webpack": true,
            "es6": true
        },
        "overrides": [
            {
                "files": [
                    "**/*.js"
                ],
                "rules": {
                    "no-console": "off"
                }
            }
        ]
    },
    "eslintIgnore": [
        "node_modules/*",
        "dist/*"
    ],
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.4.4",
        "@babel/plugin-transform-template-literals": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/runtime-corejs3": "^7.4.5",
        "@uwhealth/babel-preset-t4": "^1.0.1",
        "@uwhealth/linter-configs": "^1.5.5",
        "@uwhealth/sass-tools": "^1.1.2",
        "app-manifest-loader": "^2.4.0",
        "autoprefixer": "^9.6.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-dynamic-import-node": "^2.3.0",
        "browser-sync": "^2.26.7",
        "cache-loader": "^4.0.0",
        "chalk": "^2.4.2",
        "connect-history-api-fallback": "^1.6.0",
        "core-js": "^3.1.4",
        "css-loader": "^3.0.0",
        "cssnano": "^4.1.10",
        "del": "^4.1.1",
        "eslint": "^5.16.0",
        "extract-css-chunks-webpack-plugin": "^4.5.2",
        "extract-loader": "^3.1.0",
        "fast-glob": "^3.0.2",
        "file-loader": "^4.0.0",
        "gulp": "^4.0.2",
        "gulp-changed": "^4.0.0",
        "gulp-imagemin": "^6.0.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^8.0.0",
        "gulp-rename": "^1.4.0",
        "gulp-replace": "^1.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sizereport": "^1.2.1",
        "gulp-sourcemaps": "^2.6.5",
        "handlebars": "^4.1.2",
        "handlebars-loader": "^1.7.1",
        "hash-sum": "^1.0.2",
        "html-loader": "^0.5.5",
        "imagemin-pngquant": "^8.0.0",
        "lodash.uniq": "^4.5.0",
        "markdown-documentation-generator": "^3.2.0",
        "null-loader": "^3.0.0",
        "ora": "^3.4.0",
        "postcss-loader": "^3.0.0",
        "prerender-loader": "^1.3.0",
        "prerender-spa-plugin": "^3.4.0",
        "prettier": "^1.18.2",
        "pretty-error": "^2.1.1",
        "raw-loader": "^3.0.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^10.1.0",
        "svelte": "^3.6.1",
        "svelte-loader": "^2.13.4",
        "svelte-preprocess": "^2.14.0",
        "terser-webpack-plugin": "^1.3.0",
        "uglifyjs-webpack-plugin": "^2.1.3",
        "url-loader": "^2.0.1",
        "val-loader": "^1.1.1",
        "webpack": "^4.35.0",
        "webpack-cli": "^3.3.5",
        "webpack-hot-middleware": "^2.25.0",
        "webpack-isomorphic-compiler": "^3.1.1",
        "webpack-isomorphic-compiler-reporter": "^1.3.3",
        "webpack-isomorphic-dev-middleware": "^4.1.0",
        "webpackbar": "^3.2.0",
        "yargs": "^13.2.4"
    },
    "dependencies": {
        "escape-html": "^1.0.3",
        "promise-polyfill": "^8.1.3",
        "svelte-routing": "^1.2.0"
    },
    "readme": "./README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/UWHealth/Front-End-Framework.git"
    },
    "main": "./@src/main.js",
    "author": "UW Health Web Center",
    "license": "ISC"
}
