{{!-- Web App Settings --}}
{{#unless meta.["apple-mobile-web-app-title"]}}
<meta name="apple-mobile-web-app-title" content="UW Health">
{{/unless}}
{{#unless meta.["application-name"]}}
<meta name="application-name" content="UW Health">
{{/unless}}
{{#unless meta.["theme-color"]}}
<meta name="theme-color" content="#d40943">
{{/unless}}
{{#unless meta.['msapplication-config']}}
<meta name="msapplication-config" content="/public/browserconfig.xml">
{{/unless}}
{{#unless links.["manifest"]}}
<link rel="manifest" href="/public/manifest.webmanifest">
{{/unless}}
{{#each meta }}
    <!-- from meta -->
    <meta name="{{{@key}}}" content="{{{this}}}">
{{/each}}
{{#each links }}
    <link rel="{{{@key}}}" href="{{{this}}}">
{{/each}}
{{#each @root}}
    <!--{{@key}}-->
{{/each}}
