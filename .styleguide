{
	"sgComment": "SG",
	"devIdentifier": "[[dev]]",
	"srcFolder": "./",
	"outputFile": "./styleguide/index.html",
  "jsonOutput": "./styleguide/index.json",
	"templateFile": "./styleguide/imports/template.html",
	"themeFile": "./styleguide/imports/theme.css",
	"highlightFolder": "./node_modules/highlight.js/styles",
	"highlightStyle": "zenburn",
	"excludeDirs": [
		"target",
		"node_modules",
    "_partials/sass",
    "_partials/kits"
	],
	"fileExtensions": {
		"scss": false,
		"sass": false,
		"css": true,
		"less": false,
		"md": true
	},
	"walkerOptions": {
		"followLinks": false
	},
	"jqFile": "./js/jquery-2.1.0.js",
	"customVars": {
		"pageTitle": "Style Guide",
		"scripts": [
			"../js/plugins.js",
			"imports/hash.js",
			"imports/sticky.js"
		]
	}
}
