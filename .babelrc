{
    "presets": [
        ["env", {
            "browsers": [
                "> 1%",
                "last 2 versions",
                "chrome >= 58",
                "firefox >= 53",
                "ie >= 11",
                "safari >= 10",
                "edge >= 14"
            ],
            "loose": true
        }]
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": false,
            "loose": true
        }],
        ["syntax-dynamic-import"]
    ]

}
