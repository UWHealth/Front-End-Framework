{{!-- Web App Settings --}}
{{#unless meta.["apple-mobile-web-app-title"]}}
<meta name="apple-mobile-web-app-title" content="UW Health">
{{/unless}}
{{#unless meta.["application-name"]}}
<meta name="application-name" content="UW Health">
{{/unless}}
{{#unless meta.["theme-color"]}}
<meta name="theme-color" content="#d40943">
{{/unless}}
{{#unless browserConfig}}
<meta name="msapplication-config" content="/browserconfig.xml">
{{/unless}}
{{#unless webManifest}}
<link rel="manifest" href="/manifest.webmanifest">
{{/unless}}
{{#each meta }}
    <!-- from meta -->
    {{#if @key}}<meta name="{{{@key}}}" content="{{{this}}}">
    {{else}}<meta{{#each this}} {{{@key}}}="{{{this}}}"{{/each}}>
    {{/if}}
{{/each}}
{{#each links }}
    {{#if @key}}<link rel="{{{@key}}}" href="{{{this}}}">
    {{else}}<link{{#each this}} {{{@key}}}="{{{this}}}"{{/each}}>
    {{/if}}
{{/each}}
